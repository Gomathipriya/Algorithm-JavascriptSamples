//Using Build in method

function reverseString(str) {
  var arr = str.split('');
  arr.reverse();
  str = arr.join('');
  console.log(str);
  return str;
}

reverseString("hello");

//Without another variable

function reverseString(str){
  return str.split('').reverse().join('');
}

reverseString("hello");

//With for loop

function reverseString(str){
  var newString= "";
  for (var i = str.length - 1; i >= 0; i--) {
        newString += str[i];
    }
  return newString;
}
 
reverseString('hello');
